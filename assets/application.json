{
  "app_id": 102707,
  "title": "Kangu",
  "slug": "kangu",
  "type": "external",
  "state": "active",
  "authentication": false,
  "modules": {
    "calculate_shipping": {
      "enabled": true,
      "endpoint": "https://us-central1-ecom-kangu.cloudfunctions.net/app/ecom/modules/calculate-shipping"
    }
  },
  "auth_scope": {
    "stores/me": [
      "GET"
    ],
    "procedures": [],
    "products": [],
    "brands": [],
    "categories": [],
    "customers": [],
    "orders": [
      "GET"
    ],
    "carts": [],
    "orders/fulfillments": [
      "POST"
    ],
    "orders/payments_history": [],
    "products/quantity": [],
    "products/variations/quantity": [],
    "products/price": [],
    "products/variations/price": []
  },
  "admin_settings": {
    "zip": {
      "schema": {
        "type": "string",
        "maxLength": 9,
        "pattern": "^[0-9]{5}-?[0-9]{3}$",
        "title": "CEP de origem",
        "description": "Código postal do remetente para cálculo do frete"
      },
      "hide": true
    },
    "kangu_token": {
      "schema": {
        "type": "string",
        "maxLength": 255,
        "title": "Kangu token",
        "description": "Solitite na kangu o token para REST API cálculo frete"
      },
      "hide": true
    },
    "ordernar": {
      "schema": {
        "title": "Ordenar formas de envio",
        "type": "string",
        "description": "Escolha a ordem que as formas de envio sejam mostradas na loja",
        "enum": [
          "Opcional",
          "preco",
          "prazo"
        ],
        "default": "preco"
      },
      "hide": false
    },
    "free_shipping_rules": {
      "schema": {
        "title": "Regras de frete grátis",
        "description": "Deve ser configurado em conformidade ao que foi configurado na Kangu",
        "type": "array",
        "maxItems": 300,
        "items": {
          "title": "Regra de frete grátis",
          "type": "object",
          "minProperties": 1,
          "properties": {
            "zip_range": {
              "title": "Faixa de CEP",
              "type": "object",
              "required": [
                "min",
                "max"
              ],
              "properties": {
                "min": {
                  "type": "integer",
                  "minimum": 10000,
                  "maximum": 999999999,
                  "title": "CEP inicial"
                },
                "max": {
                  "type": "integer",
                  "minimum": 10000,
                  "maximum": 999999999,
                  "title": "CEP final"
                }
              }
            },
            "min_amount": {
              "type": "number",
              "minimum": 1,
              "maximum": 999999999,
              "title": "Valor mínimo da compra"
            }
          }
        }
      },
      "hide": false
    },
    "posting_deadline": {
      "schema": {
        "title": "Prazo de postagem",
        "type": "object",
        "required": [
          "days"
        ],
        "additionalProperties": false,
        "properties": {
          "days": {
            "type": "integer",
            "minimum": 0,
            "maximum": 999999,
            "title": "Número de dias",
            "description": "Dias de prazo para postar os produtos após a compra"
          },
          "working_days": {
            "type": "boolean",
            "default": true,
            "title": "Dias úteis"
          },
          "after_approval": {
            "type": "boolean",
            "default": true,
            "title": "Após aprovação do pagamento"
          }
        }
      },
      "hide": false
    },
    "disable_auto_tag": {
      "schema": {
        "type": "boolean",
        "default": false,
        "title": "Desabilitar geração de envios a Kangu",
        "description": "Desativa a criação automática de tags de envio para Kangu"
      },
      "hide": false
    }
  },
  "auth_callback_uri": "https://us-central1-ecom-kangu.cloudfunctions.net/app/ecom/auth-callback",
  "version": "1.0.0"
}